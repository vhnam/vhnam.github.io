{
  "name": "vhnam.github.io",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass:watch" : "sass --watch src/assets/scss:src/assets/css",
    "sass:build": "sass --update src/assets/scss:src/assets/css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
